        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/fmtrpcBakeMacaroonRequest"
            }
          }
        ],
        "tags": [
          "Fmt"
        ]
      }
    },
    "/v1/stop": {
      "post": {
        "summary": "fmtcli: `stop`\nStopDaemon will send a shutdown request to the interrupt handler, triggering\na graceful shutdown of the daemon.",
