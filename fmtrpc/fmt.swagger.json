          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/fmtrpcStopRequest"
            }
          }
        ],
        "tags": [
          "Fmt"
        ]
      }
    },
    "/v1/test": {
      "get": {
        "summary": "fmtcli: `test`\nTestCommand will send a string response regardless if a macaroon is provided or not.",
        "operationId": "Fmt_TestCommand",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
